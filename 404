<script>
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
        show_content();
        $('#category_container, #category_container2').prepend('<li href="#" class="show_all_stores">All</li>');
        show_cat_stores();
    })
    
    function renderPage(){
        var stores = getStoresList();
        renderStoreList('#store_list_container','#store_list_template', stores, "stores", "A", "M");
        renderStoreList('#store_list_container2','#store_list_template2', stores, "stores","M", "Z" );
        var categories = getStoreCategories();
        renderGeneral('#category_container, #category_container2', '#category_template', categories);
    }
</script>